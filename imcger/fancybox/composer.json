{
	"name": "imcger/fancybox",
	"type": "phpbb-extension",
	"description": "Insert the images viewer Fancybox. Fancybox 3 licensed GPLv3 for open source use. http://fancyapps.com/fancybox/",
	"homepage": "https://github.com/IMC-GER/phpBB-Fancybox",
	"version": "1.3.1",
	"time": "2023-07-30",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "Thorsten Ahlers",
			"homepage": "https://github.com/IMC-GER",
			"role": "Developer"
		}
	],
    "require": {
        "php": ">=7.4,<8.5.0@dev",
        "composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "Fancybox",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		},
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/IMC-GER/phpBB-Fancybox/main",
			"filename": "fancybox_version.json",
			"ssl": true
		}
	}
}
